<h1> Login </h1>

<!-- <%= form_for(@user) do |f| %> -->
  <%= form_tag("/sessions", method: "post") do %>
    <div class="field">
      <%= f.label :username %><br>
      <%= f.text_field :username, :placeholder => "username..." %>
    </div>
    <div class="field">
      <%= f.label :password %><br>
      <%= f.password_field :password, :placeholder => "password..." %>
    </div>
    <div class="actions">
      <%= f.submit("Login") %>
    </div>
  <% end %>
<!-- <% end %> -->


